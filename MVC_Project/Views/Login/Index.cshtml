@model Login
@{ ViewData["Title"] = "Login"; }

<h2>Login</h2>
<form asp-action="Index" method="post">
    <div>
		<label asp-for="UserName"></label>
    </div>
	<div>
		<input asp-for="UserName" />
	</div>
    <div>
		<label asp-for="Password"></label>
    </div>
	<div>
		<input asp-for="Password" />
	</div>
	<input type="submit" value="Login" />
</form>

